<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SwControler.MainControl">
    <children>
        <BorderPane fx:id="borderPane" minHeight="600.0" minWidth="800.0" prefHeight="678.0" prefWidth="1360.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <top>
                <MenuBar maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="1360.0" BorderPane.alignment="CENTER">
                    <menus>
                        <Menu mnemonicParsing="false" text="Arquivo">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Novo" />
                        <MenuItem mnemonicParsing="false" text="Abrir" />
                        <MenuItem mnemonicParsing="false" text="Salvar" />
                        <MenuItem mnemonicParsing="false" text="Salvar Como" />
                        <MenuItem mnemonicParsing="false" text="Sair" />
                        <MenuItem mnemonicParsing="false" text="Imprimir" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Editar">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Desfazer" />
                        <MenuItem mnemonicParsing="false" text="Refazer" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Opções">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Ajuda">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#aboutMenuAction" text="Sobre o Switcher" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <right>
                <VBox id="rightVBox" alignment="TOP_CENTER" disable="false">
                    <children>
                        <Button fx:id="computerButton" disable="true" mnemonicParsing="false" onAction="#computerButtonAction" />
                        <Button fx:id="printerButton" disable="true" mnemonicParsing="false" onAction="#printerButtonAction" />
                        <Button fx:id="phoneButton" disable="true" mnemonicParsing="false" onAction="#phoneButtonAction" />
                        <Button fx:id="switchButton" disable="true" mnemonicParsing="false" onAction="#switchButtonAction" />
                        <Button fx:id="hubButton" disable="true" mnemonicParsing="false" onAction="#hubButtonAction" />
                        <Button fx:id="lineButton" contentDisplay="CENTER" disable="true" mnemonicParsing="false" onAction="#lineButtonAction" />
                    </children>
                </VBox>
            </right>
            <left>
                <VBox BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="editButton" mnemonicParsing="false" onAction="#editButtonAction" />
                        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteButtonAction" />
                        <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonAction" />
                        <Button fx:id="reportButton" mnemonicParsing="false" onAction="#reportButtonAction" />
                        <Button fx:id="playButton" mnemonicParsing="false" onAction="#playButtonAction" />
                    </children>
                </VBox>
            </left>
         <center>
            <ScrollPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
        </BorderPane>
    </children>
</AnchorPane>
